<div class="tabela">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Sala</th>
        <th scope="col">Professor</th>
        <th scope="col">Turno</th>
        <th scope="col"></th>
        <th scope="col"></th>
      </tr>
    </thead>
    <tbody>
      <% lab.forEach(function(labo, index){ %>
      <tr>
        <th scope="row"><%= index %></th>
        <td><%= labo.sala %></td>
        <td><%= labo.professor %></td>
        <td><%= labo.turno %></td>
        <td>
          <a href="/laboratorio/delete/<%= labo._id %>">
            <img src="img/trash.svg" alt="lixo" width="20" height="20" />
          </a>
        </td>
        <td><%- include('./modal', {labo: labo}) %></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>